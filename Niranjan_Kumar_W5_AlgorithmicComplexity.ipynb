    {
      "cell_type": "code",
      "source": [
        "def reverse(lst):\n",
        "  n = len(lst)\n",
        "  answer = []\n",
        "  for i in range(n-1, -1, -1):\n",
        "    answer.append(lst[i])\n",
        "  return answer\n",
        "\n",
        "reversed_ls1 = reverse(ls1)\n",
        "print(reversed_ls1)\n",
        "print(ls1)"
      ],
      "metadata": {
        "id": "mwhy3i18-jHc"
