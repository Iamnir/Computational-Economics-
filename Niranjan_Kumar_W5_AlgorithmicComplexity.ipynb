        "def gcd(a, b):\n",
        "  if a >= b:\n",
        "    large, small = a, b\n",
        "  else:\n",
        "    large, small = b, a\n",
        "  \n",
        "  mod = large % small\n",
        "\n",
        "  if mod == 0:\n",
        "    return small\n",
        "  else:\n",
        "    return gcd(small, mod)\n",
        "\n",
        "gcd(16, 36), gcd(45, 72)\n"
